# Runtime
NODE_ENV=development
PORT=8080

# Authentication
ADMIN_API_KEY=change-admin-key
INTERNAL_API_KEY=change-internal-key
# Database (PostgreSQL)
DATABASE_URL=postgres://postgres:postgres@localhost:5432/algohire_webhook

# Redis
REDIS_URL=redis://localhost:6379

# Security
HMAC_GLOBAL_FALLBACK_SECRET=change-hmac-secret

# Delivery
MAX_RETRIES=5
QUEUE_CONCURRENCY=10
REQUEST_TIMEOUT_MS=8000

# Service identity
SERVICE_NAME=algohire-webhook-relay
